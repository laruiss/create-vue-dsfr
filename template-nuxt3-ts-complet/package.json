{
  "name": "vue-dsfr-nuxt-ts-starter",
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "format": "eslint . --fix",
    "generate": "nuxt generate",
    "lint": "eslint .",
    "preview": "nuxt preview",
    "test": "run-s test:unit test:e2e",
    "test:unit": "vitest run",
    "postinstall": "nuxt prepare",
