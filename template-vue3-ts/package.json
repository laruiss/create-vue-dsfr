{
  "name": "vue-dsfr-project",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean:dist": "rimraf dist",
    "build": "run-p clean:dist type-check build-only",
    "build-only": "vite build",
    "dev": "vite",
    "format": "run-s format:prettier format:eslint",
    "format:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "format:prettier": "prettier --write src/",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "pretest:e2e": "npm run build",
    "pretest:e2e:ci": "npm run build",
    "preview": "vite preview --host --port 5050",
    "test": "run-s test:unit test:ct:ci build test:e2e:ci",
    "test:ct": "cypress open --component",
    "test:ct:ci": "cypress run --component",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@gouvfr/dsfr": "^1.9.3",
    "@gouvminint/vue-dsfr": "^4.0.2",
    "focus-trap": "^7.4.3",
    "focus-trap-vue": "^4.0.2",
    "oh-vue-icons": "1.0.0-rc3",
    "pinia": "^2.1.4",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@cypress/vue": "^5.0.5",
    "@rushstack/eslint-patch": "^1.3.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^7.0.0",
    "@tsconfig/node18": "^18.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.19",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.4.0",
    "cypress": "^12.16.0",
    "eslint": "^8.44.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.15.1",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "start-server-and-test": "^2.0.0",
    "typescript": "~5.1.6",
    "unplugin-auto-import": "^0.16.4",
    "vite": "^4.3.9",
    "vite-plugin-pwa": "^0.16.4",
    "vitest": "^0.32.0",
    "vue-tsc": "^1.8.3",
    "workbox-window": "^7.0.0"
  }
}